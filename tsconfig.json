{
	"compilerOptions": {
		"target": "es5",
		"module": "commonjs",
		"strict": true,
		"strictNullChecks": false,
		"esModuleInterop": true,
		"experimentalDecorators": true,
		"emitDecoratorMetadata": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"outDir": "dist/"
	}
}
